{
  "name": "bevelog_client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.33",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "jest --config ./conf/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "eject": "react-scripts eject",
    "serve": "webpack serve --config ./conf/webpack.development.conf.js",
    "build": "webpack --config ./conf/webpack.production.conf.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@storybook/addon-actions": "^6.4.0-beta.31",
    "@storybook/addon-essentials": "^6.4.0-beta.31",
    "@storybook/addon-links": "^6.4.0-beta.31",
    "@storybook/builder-webpack5": "^6.4.0-beta.31",
    "@storybook/manager-webpack5": "^6.4.0-beta.31",
    "@storybook/node-logger": "^6.4.0-beta.31",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.0-beta.31",
    "@testing-library/react": "^11.2.7",
    "@types/lodash": "^4.14.175",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.3.1",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.4.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "^27.3.1",
    "lint-staged": "^11.2.6",
    "mini-css-extract-plugin": "^2.4.2",
    "prettier": "2.4.1",
    "react-router-dom": "^5.3.0",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-merge": "^5.8.0",
    "webpack-virtual-modules": "^0.4.3"
  }
}
